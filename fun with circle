import turtle
a=turtle.Turtle()
a.fillcolor("black")
a.begin_fill()
a.circle(10)
a.end_fill()
a.up()
a.goto(40,20)
a.down()
a.fillcolor("orange")
a.begin_fill()
a.circle(10)
a.end_fill()
a.up()
a.goto(-80,20)
a.down()
a.fillcolor("blue")
a.begin_fill()
a.circle(10)
a.end_fill()
a.up()
a.goto(-80,-20)
a.down()
a.fillcolor("red")
a.begin_fill()
a.circle(10)
a.end_fill()
a.up()
a.goto(80,-20)
a.down()
a.fillcolor("black")
a.begin_fill()
a.circle(10)
a.end_fill()
